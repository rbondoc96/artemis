{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "rootDir": ".",
        "outDir": "build",
        "removeComments": true,
        "moduleResolution": "node",
        "allowJs": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": [
        "**.ts",
        "./**/*",
    ],
    "exclude": [
        "node_modules",
        "frontend",
        "build"
    ]
}